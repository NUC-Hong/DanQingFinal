
page{

  font-size: 16px;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
  width: 750rpx;
  height: 540rpx;
  background-image: url('https://guhua-buc-1324634892.cos.ap-shanghai.myqcloud.com/static/background.jpg?sign=9269896da9f404de8185327fd5fead0f&t=1655884097');

}
.page__hd {
  padding: 30px;
}


.page__title {
  text-align: left;
  font-size: 20px;
  font-weight: 400;
}

.page__desc {
  margin-top: 5px;
  color: #888888;
  text-align: left;
  font-size: 14px;
}
.top {
  font-family: 'Courier New', Courier, monospace;
  text-align: center;
  font-size: 50rpx;
}
.con_list{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
}
.con_list .list{
  width:45%;
  height: auto;
  text-align: center;
  margin-top: 40rpx;

}
.img_list{
  border:1px solid gray;
  border-radius: 20%;
}
.img{
  width:500rpx;
  border-radius: 20%;
}
